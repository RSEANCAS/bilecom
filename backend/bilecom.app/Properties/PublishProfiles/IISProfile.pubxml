            <Source MatchValue="Data Source=172.107.175.145; Initial Catalog=dbBileCom; User Id=sa;Password=abcDEF123*" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
