    <UserName>jarevsac-001</UserName>
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="Local" Order="1" Enabled="False">
          <Destination Path="" />
